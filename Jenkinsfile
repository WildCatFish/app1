pipeline {
    agent { docker 'maven:3.8.2' }

    stages {
        stage ('Compile Stage') {

            steps {
                sh 'mvn --version'
                }
            }
        }

        stage ('Testing Stage') {

            steps {
                sh 'mvn --version'
                }
            }
        }


        stage ('Deployment Stage') {
            steps {
                sh 'mvn --version'
                }
            }
        }
    }
}